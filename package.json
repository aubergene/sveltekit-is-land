{
	"name": "sveltekit-is-land",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "npm run islands:build && vite build",
		"preview": "vite preview",
		"islands:build": "vite build -c islands.config.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@11ty/is-land": "^2.0.3",
		"@lukeed/uuid": "^2.0.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"devalue": "^3.1.3",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module"
}
